<?xml version="1.0" encoding="utf-8"?>
<data-extraction-rules>
    <cloud-backup>
        <!-- Keep account credentials and API keys out of backup/transfer payloads. -->
        <exclude domain="file" path="datastore/auth.preferences_pb" />
        <exclude domain="file" path="datastore/settings.preferences_pb" />
    </cloud-backup>
    <device-transfer>
        <exclude domain="file" path="datastore/auth.preferences_pb" />
        <exclude domain="file" path="datastore/settings.preferences_pb" />
    </device-transfer>
</data-extraction-rules>
